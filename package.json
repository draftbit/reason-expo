{
  "name": "reason-expo",
  "description": "ReasonML bindings for Expo",
  "version": "30.3.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean-build": "bsb -clean-world -make-world",
    "install::dev": "yarn && install-self-peers -- --ignore-scripts",
    "pub": "npx np --yolo"
  },
  "license": "MIT",
  "keywords": [
    "reason",
    "reasonml",
    "bucklescript",
    "react-native",
    "expo"
  ],
  "homepage": "https://github.com/fiber-god/reason-expo.git",
  "author": "Juwan Wheatley",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiber-god/reason-expo.git"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": "^1.1.1",
    "bs-platform": "^4.0.0"
  },
  "bugs": "https://github.com/fiber-god/reason-expo/issues",
  "peerDependencies": {
    "bs-react-native": "^0.8.0",
    "expo": "^30.0.0",
    "reason-react": "^0.4.2"
  }
}
